<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Schedule">

	<select id="findByUser" parameterType="integer" resultType="Schedule">
	SELECT * FROM schedules where fk_creater = #{id}
	</select>
	
	<insert id="insertSchedule" parameterType="Schedule" useGeneratedKeys="true"   
  		keyProperty="seq">
		insert into schedules (fk_creater, fk_places, starts_at, when_registered)
				Values(#{owner.seq},#{place.seq},#{time}, #{registeredTime});
	</insert>
</mapper>